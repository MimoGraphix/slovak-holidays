{
    "name": "rekurzia/slovak-holidays",
    "type": "library",
    "description": "Simple PHP library/helper for getting Slovak holidays",
    "keywords": ["holidays", "slovak"],
    "homepage": "https://github.com/rekurzia/slovak-holidays",
    "license": "MIT",
    "require": {
        "php": ">=5.4.0"
    },
    "authors": [
        {
            "name": "Vladimír Kriška",
            "email": "ujovlado@gmail.com"
        }
    ],
    "autoload": {
        "classmap": ["src/"]
    },
    "require-dev": {
        "nette/tester": "~1.5.0",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "scripts": {
        "phpcs": "phpcs -n --ignore=vendor,tests --extensions=php --standard=psr2 .",
        "phpcbf": "phpcbf -n --ignore=vendor,tests --extensions=php --standard=psr2 ."
    }
}
